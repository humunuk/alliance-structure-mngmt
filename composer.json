{
    "name": "humunuk/alliance-structure-mngmt",
    "description": "Simple SEAT package to get an overview of alliance structures",
    "homepage": "https://github.com/humunuk/alliance-structure-mngmt",
    "authors": [
        {
            "name": "Siim Kallari",
            "email": "siim.kallari@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Humunuk\\Seat\\AllianceStructureManagement\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
        "eveseat/services": "^5.0",
        "eveseat/web": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Humunuk\\Seat\\AllianceStructureManagement\\AllianceStructureManagementServiceProvider"
            ]
        }
    },
    "require-dev": {
        "rector/rector": "^0.18.3",
        "driftingly/rector-laravel": "^0.24.1"
    }
}
